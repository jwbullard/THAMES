<?xml version="1.0" encoding="UTF-8"?>
<chemistry_data xmlns:xs="http://www.w3.org/2001/XMLSchema-instance"
    xs:noNamespaceSchemaLocation="/Users/bullard/Software/MyProjects/THAMES/src/xsdfiles/chemistry.xsd">
  <!--The number of possible phases in the microstructure not counting Void-->
  <numentries>5</numentries>
  <!--Temperature in K-->
  <temperature>298.15</temperature>
  <!--Do not change next value, used for Parrott and Killoh kinetic model-->
  <reftemperature>296.15</reftemperature>
  <!--Set to 1 for saturated, 0 for sealed-->
  <saturated>0</saturated>
  <!--Information for initial solution IC composition besides water, each in mol/kgw-->
  <solution>
      <ICcomp>
          <name>Ca</name>
          <conc>0.001</conc>
      </ICcomp>
      <ICcomp>
          <name>K</name>
          <conc>0.001</conc>
      </ICcomp>
      <ICcomp>
          <name>S</name>
          <conc>0.0005</conc>
      </ICcomp>
      <ICcomp>
          <name>O</name>
          <conc>0.004</conc>
      </ICcomp>
      <ICcomp>
          <name>H</name>
          <conc>0.002</conc>
      </ICcomp>
  </solution>

  <!--Void phase should always be present and first in the list-->
  <phase>
    <thamesname>Void</thamesname>
    <!--Unique integer id for microstructure phase-->
    <id>0</id>
    <!--Interface data not used for void phase-->
    <interface_data>
      <randomgrowth>0.0</randomgrowth>
    </interface_data>
    <!--Display rgb colors, 0 to 255-->
    <display_data>
        <red>0.0</red>
        <green>0.0</green>
        <blue>0.0</blue>
        <gray>0.0</gray>
    </display_data>
  </phase>
  <!--Water phase should always be present and  in the list-->
  <phase>
    <thamesname>Electrolyte</thamesname>
    <!--Which GEM phase(s) and DC(s) are linked to this microstructure phase?-->
    <gemphase_data>
      <gemphasename>aq_gen</gemphasename>
      <gemdc>
        <gemdcname>H2O@</gemdcname>
        <gemdcporosity>1.0</gemdcporosity>
      </gemdc>
    </gemphase_data>
    <id>1</id>
    <!--Interface data not used for void phase-->
    <interface_data>
      <randomgrowth>0.0</randomgrowth>
    </interface_data>
    <display_data>
      <red>25.0</red>
      <green>25.0</green>
      <blue>25.0</blue>
      <gray>25.0</gray>
    </display_data>
  </phase>
  <phase>
    <thamesname>Alite</thamesname>
    <gemphase_data>
      <gemphasename>Alite</gemphasename>
      <gemdc>
        <gemdcname>C3S</gemdcname>
      </gemdc>
    </gemphase_data>
    <id>2</id>
    <!--Solid phases have meaningful interface data-->
    <interface_data>
        <!--randomgrowth between 0 and 1, 1 is totally random attachment
            or detachment, 0 means interface mean curvature governs the
        attachment and detachment points-->
      <randomgrowth>0.0</randomgrowth>
      <!--Affinity is the relative strength of the attraction between this phase
          and a template phase.  If a template is not listed, it takes the default
          value of 0.  Positive numbers indicate attraction and negative numbers
      mean repulsion-->
      <affinity>
        <affinityphase>Alite</affinityphase>
        <affinityvalue>1</affinityvalue>
      </affinity>
    </interface_data>
    <!--We can specify minority impurity concentrations in a solid phase, which
    will be released in proportion during dissolution-->
    <impurity_data>
        <k2ocoeff>0.00087</k2ocoeff>  <!--K2O mole fraction-->
        <na2ocoeff>0.0</na2ocoeff>    <!--Na2O mole fraction-->
        <mgocoeff>0.00861</mgocoeff>    <!--MgO mole fraction-->
        <so3coeff>0.007942</so3coeff>   <!--SO3 mole fraction-->
    </impurity_data>
    <kinetic_data>
      <!--Type is whether the phase is controlled by kinetics or thermodynamics-->
      <type>ParrotKilloh</type>
      <specificSurfaceArea>402.0</specificSurfaceArea>
      <refSpecificSurfaceArea>385.0</refSpecificSurfaceArea>

      <!--k1, k2, k3, n1, n3 are Parrott and Killoh parameters. Do not change-->
      <k1>1.5</k1>
      <k2>0.05</k2>
      <k3>1.1</k3>
      <n1>0.7</n1>
      <n3>3.3</n3>
      <!--Ea is the apparent activation energy for dissolution-->
      <Ea>41570.0</Ea>
      <!--critdoh is another P&K parameter-->
      <critdoh>2.0</critdoh>
    </kinetic_data>
    <display_data>
      <red>162.0</red>
      <green>117.0</green>
      <blue>95.0</blue>
      <gray>220.0</gray>
    </display_data>
  </phase>
  <phase>
    <thamesname>Portlandite</thamesname>
    <gemphase_data>
    <gemphasename>Portlandite</gemphasename>
    <gemdc>
      <gemdcname>Portlandite</gemdcname>
    </gemdc>
    </gemphase_data>
    <id>3</id>
    <interface_data>
      <randomgrowth>0.6</randomgrowth>
      <affinity>
        <affinityphase>Portlandite</affinityphase>
        <affinityvalue>1</affinityvalue>
      </affinity>
      <affinity>
          <affinityphase>CSHQ</affinityphase>
        <affinityvalue>1</affinityvalue>
      </affinity>
      <affinity>
        <affinityphase>Alite</affinityphase>
        <affinityvalue>-10</affinityvalue>
      </affinity>
    </interface_data>
    <display_data>
      <red>0.0</red>
      <green>0.0</green>
      <blue>255.0</blue>
      <gray>186.0</gray>
    </display_data>
  </phase>
  <phase>
    <thamesname>CSHQ</thamesname>
    <gemphase_data>
      <gemphasename>CSHQ</gemphasename>
      <gemdc>
        <gemdcname>CSHQ-JenD</gemdcname>
        <gemdcporosity>0.4935</gemdcporosity>
      </gemdc>
      <gemdc>
        <gemdcname>CSHQ-JenH</gemdcname>
        <gemdcporosity>0.4935</gemdcporosity>
      </gemdc>
      <gemdc>
        <gemdcname>CSHQ-TobD</gemdcname>
        <gemdcporosity>0.2004</gemdcporosity>
      </gemdc>
      <gemdc>
        <gemdcname>CSHQ-TobH</gemdcname>
        <gemdcporosity>0.2004</gemdcporosity>
      </gemdc>
      <gemdc>
        <gemdcname>KSiOH</gemdcname>
        <gemdcporosity>0.1825</gemdcporosity>
      </gemdc>
      <gemdc>
        <gemdcname>NaSiOH</gemdcname>
        <gemdcporosity>0.1825</gemdcporosity>
      </gemdc>
    </gemphase_data>
    <id>4</id>
    <interface_data>
      <randomgrowth>0.995</randomgrowth>
      <affinity>
        <affinityphase>CSHQ</affinityphaseid>
        <affinityvalue>1</affinityvalue>
      </affinity>
      <affinity>
        <affinityphaseid>Alite</affinityphaseid>
        <affinityvalue>4</affinityvalue>
      </affinity>
    </interface_data>
    <poresizefilename>CSH-PoreSizeDistribution.csv</poresizefilename>
    <Rd>
      <Rdelement>K</Rdelement>
      <Rdvalue>0.42</Rdvalue>
    </Rd>
    <Rd>
      <Rdelement>Na</Rdelement>
      <Rdvalue>0.42</Rdvalue>
    </Rd>
    <display_data>
      <red>245.0</red>
      <green>222.0</green>
      <blue>179.0</blue>
      <gray>159.0</gray>
    </display_data>
  </phase>
</chemistry_data>
