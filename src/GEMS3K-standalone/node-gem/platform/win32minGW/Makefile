####### Build rules

all: $(OBJECTS_DIR) $(TARGET)

$(TARGET):  $(OBJECTS)  
	$(LINK) $(LFLAGS) -o $(TARGET) object_ld_script $(LIBS)

$(OBJECTS_DIR):
